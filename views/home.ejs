<script>
  console.log('Script loaded.');

  const form = document.querySelector('form');
  const successMessageDiv = document.querySelector('.success-popup');
  const button = document.querySelector('.centered-button');

  form.addEventListener('submit', async (event) => {
    event.preventDefault();
    console.log('Form submit button clicked.');

    // Get form field values
    const name = document.getElementById('name').value;
    const email = document.getElementById('email').value;
    const subject = document.getElementById('subject').value;
    const message = document.getElementById('message').value;

    // Create an object with the form data
    const formData = {
      name,
      email,
      subject,
      message,
    };

    // Disable the submit button while processing
    button.disabled = true;

    try {
      const response = await fetch('/send-message', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json', // Set the content type to JSON
        },
        body: JSON.stringify(formData), // Convert the object to JSON string
      });

      const data = await response.json();

      if (response.status === 200) {
        // Show success message and hide other messages
        if (successMessageDiv) {
          successMessageDiv.style.display = 'block';
        }
       
        form.reset(); // Reset the form fields after successful submission

        // Show SweetAlert2 success message
        Swal.fire({
          icon: 'success',
          title: 'Success!',
          text: 'Message sent successfully. Thank you!',
        });
      } else {
        // Show error message and hide other messages
        
        if (successMessageDiv) {
          successMessageDiv.style.display = 'none';
        }
      }
    } catch (error) {
      // Show error message and hide other messages
      
      if (successMessageDiv) {
        successMessageDiv.style.display = 'none';
      }
    }

    // Re-enable the submit button
    button.disabled = false;

    console.log('Form submitted.');
  });

  // Function to show SweetAlert2 success message when the button is clicked
  function handleButtonClick() {
    Swal.fire({
      icon: 'success',
      title: 'Success!',
      text: 'Message sent successfully. Thank you!',
    });
  }

  // Add click event listener to the button
  button.addEventListener('click', handleButtonClick);
</script>

           

