        </ul>
        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav><!-- .navbar -->

    </div>
  </header><!-- End Header -->

  <main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <ol>
          <li><a href="/">Home</a></li>
          <li>Messages Details</li>
        </ol>
        <h2>Messages Details</h2>

      </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Portfolio Details Section ======= -->
    <section id="portfolio-details" class="portfolio-details">
      <div class="container">

        <div class="row gy-4">


            <div class="col-lg-5">
                <div class="portfolio-info">
                  <h3>All Messages</h3>
                  <% if (messages.length > 0) { %>
                    <% messages.forEach(message => { %>
                      <div class="message-item">
                        <p><strong>Name:</strong> <%= message.name %></p>
                        <p><strong>Email:</strong> <%= message.email %></p>
                        <p><strong>Subject:</strong> <%= message.subject %></p>
                        <p><strong>Message:</strong> <%= message.message %></p>
                      </div>
                    <% }) %>
                  <% } else { %>
                    <p>No messages found.</p>
                  <% } %>
                </div>
              </div>
              

              <div class="col-lg-2">
                </div>

          
          <div class="col-lg-5">
            <div class="portfolio-details-slider swiper">
              <div class="swiper-wrapper align-items-center">

                <div class="swiper-slide">
                  <img src="main-template/img/portfolio/portfolio-1.jpg" alt="">
                </div>

                <div class="swiper-slide">
                  <img src="main-template/img/portfolio/portfolio-2.jpg" alt="">
                </div>

                <div class="swiper-slide">
                  <img src="main-template/img/portfolio/portfolio-3.jpg" alt="">
                </div>

              </div>
              <div class="swiper-pagination"></div>
            </div>
          </div>

        </div>

      </div>
    </section><!-- End Portfolio Details Section -->

  </main><!-- End #main -->



