{
    "name": "enigma",
    "version": "1.0.5",
    "description": "TailwindCSS HTML Admin Template",
    "author": "Left4code",
    "private": true,
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "watch": "concurrently \"postcss ./src/css/app.css -o ./dist/css/_app.css --watch\" \"mix watch\"",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "postcss ./src/css/app.css -o ./dist/css/_app.css && mix --production"
    },
